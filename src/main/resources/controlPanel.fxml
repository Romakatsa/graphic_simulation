      <Slider fx:id="slider2" layoutX="69.0" layoutY="123.0" onMouseDragged="#dragSlider2" onMouseReleased="#doneDragSlider2" orientation="VERTICAL" />
      <Slider fx:id="slider3" layoutX="106.0" layoutY="123.0" onDragDetected="#dragSlider3" onDragDone="#doneDragSlider3" orientation="VERTICAL" />
      <Slider fx:id="slider4" layoutX="143.0" layoutY="123.0" onDragDetected="#dragSlider4" onDragDone="#doneDragSlider4" orientation="VERTICAL" />
      <Slider fx:id="slider5" layoutX="180.0" layoutY="123.0" onDragDetected="#dragSlider5" onDragDone="#doneDragSlider5" orientation="VERTICAL" />
      <ToggleButton layoutX="159.0" layoutY="27.0" mnemonicParsing="false" text="ToggleButton" />
      <ChoiceBox fx:id="selectedDriver" layoutX="39.0" layoutY="81.0" prefWidth="150.0" />
